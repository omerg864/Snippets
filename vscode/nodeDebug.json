{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Debug with Nodemon",
			"runtimeExecutable": "nodemon",
			"runtimeArgs": ["--exec", "ts-node", "server/server.ts"],
			"restart": true,
			"console": "integratedTerminal",
			"internalConsoleOptions": "neverOpen"
		}
	]
}
